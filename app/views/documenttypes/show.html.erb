<div class="page-header">
  <h3>Documenttype #<%= @documenttype.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @documenttype.name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/documenttypes" class="btn btn-primary">
        Back
      </a>
      <a href="/documenttypes/<%= @documenttype.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_documenttype/<%= @documenttype.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
