<div class="page-header">
  <h1>
    Servicetypes
    <a href="/servicetypes/new" class="btn btn-lg btn-success">New Servicetype</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>

      <% @servicetypes.each do |servicetype| %>
      <tr>
        <td><%= servicetype.name %></td>
        <td>
          <a href="/servicetypes/<%= servicetype.id %>" class="btn btn-primary">Show</a>
          <a href="/servicetypes/<%= servicetype.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_servicetype/<%= servicetype.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
